{
  "name": "task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "vite": "^5.1.4"
  },
  "dependencies": {
    "alchemy-sdk": "^3.1.2",
    "jest": "^29.7.0"
  },
  "vercel": {
    "builds": [
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
}
